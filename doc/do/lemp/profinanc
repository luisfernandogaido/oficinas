server {
    server_name testeatlas.profinanc.com.br;
    root /var/www/html/profinanc/testeatlas;
	index index.html index.php;
	client_max_body_size 20M;
	location / {
	    try_files $uri $uri/ =404;
	}
    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
  	    access_log /var/log/nginx/testeatlas-access.log combined;
  	    error_log /var/log/nginx/testeatlas-error.log;
    }
    location ~* \.(js|css|png|jpg|jpeg|gif|svg|ico)$ {
        expires 15m;
        add_header Cache-Control "public, no-transform";
    }
    fastcgi_read_timeout 600;
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/testeatlas.profinanc.com.br/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/testeatlas.profinanc.com.br/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}
server {
    if ($host = testeatlas.profinanc.com.br) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    server_name testeatlas.profinanc.com.br;
    listen 80;
    return 404; # managed by Certbot
}

