server {
    listen 80
    default_server;
    server_name _;
    root /var/www/html;
    index index.html index.php;
    location / {
        try_files $uri $uri/ =404;
    }
}

server {
    server_name files.gaido.dev;
    root /var/www/html/files;
    index index.html;
    client_max_body_size 2048M;
	location / {
	    try_files $uri $uri/ =404;
	}
    sendfile on;
    sendfile_max_chunk 1m;
    tcp_nopush on;
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/files.gaido.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/files.gaido.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

server {
    if ($host = files.gaido.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    server_name files.gaido.dev;
    listen 80;
    return 404; # managed by Certbot
}

server {
    server_name monitorlegislativo.com.br;
    root /var/www/html/federal;
    index index.html index.php;
    client_max_body_size 20M;
    location / {
        try_files $uri $uri/ =404;
    }
    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
        access_log /var/log/nginx/federal-access.log combined;
        error_log /var/log/nginx/federal-error.log;
    }
    location ~* \.(js|css|png|jpg|jpeg|gif|svg|ico)$ {
        expires 15m;
        add_header Cache-Control "public, no-transform";
    }
    # rewrite ^/home/(.*)$ /app/home.php?u=$1 last;
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/monitorlegislativo.com.br/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/monitorlegislativo.com.br/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

server {
    if ($host = monitorlegislativo.com.br) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    listen 80;
    server_name monitorlegislativo.com.br;
    return 404; # managed by Certbot
}

server {
    server_name consorcio.profinanc.com.br;
    root /var/www/html/consorcio;
    index index.html index.php;
    client_max_body_size 1024M;
    location / {
        try_files $uri $uri/ =404;
    }
    proxy_read_timeout 1800;
    proxy_connect_timeout 1800;
    proxy_send_timeout 1800;
    send_timeout 1800;
    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
        access_log /var/log/nginx/consorcio-access.log combined;
        error_log /var/log/nginx/consorcio-error.log;
        fastcgi_param STORAGE_URL "https://drive.profinanc.com.br";
        fastcgi_param STORAGE_TOKEN "NDBd84dWpfGg";
    }
    location ~* \.(js|css|png|jpg|jpeg|gif|svg|ico)$ {
        expires 15m;
        add_header Cache-Control "public, no-transform";
    }
    # rewrite ^/home/(.*)$ /app/home.php?u=$1 last;

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/consorcio.profinanc.com.br/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/consorcio.profinanc.com.br/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

server {
    if ($host = consorcio.profinanc.com.br) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    listen 80;
    server_name consorcio.profinanc.com.br;
    return 404; # managed by Certbot
}

