server {
	server_name godonto.gaido.dev;
	location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $remote_addr;
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:4015;

        proxy_read_timeout 1800;
        proxy_connect_timeout 1800;
        proxy_send_timeout 1800;
        send_timeout 1800;
                
  	    access_log /var/log/nginx/godonto-access.log combined;
  	    error_log /var/log/nginx/godonto-error.log;
	}
    client_max_body_size 64M;
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/godonto.gaido.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/godonto.gaido.dev/privkey.pem; # managed by Certbot
