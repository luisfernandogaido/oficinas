server {
	server_name gmail.gaido.dev;
	location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $remote_addr;
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:4007;
  	    access_log /var/log/nginx/gmail-access.log combined;
  	    error_log /var/log/nginx/gmail-error.log;
	}
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/gmail.gaido.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/gmail.gaido.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}
server {
    if ($host = gmail.gaido.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    listen 80;
	server_name gmail.gaido.dev;
    return 404; # managed by Certbot
}

