server {
    if ($host = gd.gaido.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
	server_name gd.gaido.dev;
    listen 80;
    return 404; # managed by Certbot
}

server {
    server_name pro-go2.profinanc.com.br;
    location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $remote_addr;
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:4029;
        access_log /var/log/nginx/pro-go2-access.log combined;
        error_log /var/log/nginx/pro-go2-error.log;
    }
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/pro-go2.profinanc.com.br/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/pro-go2.profinanc.com.br/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

server {
    if ($host = pro-go2.profinanc.com.br) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    server_name pro-go2.profinanc.com.br;
    listen 80;
    return 404; # managed by Certbot
}

server {
    server_name machinelearning.gaido.dev;
    location / {
        proxy_pass http://127.0.0.1:8001;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        access_log /var/log/nginx/machinelearning-access.log combined;
        error_log /var/log/nginx/machinelearning-error.log;
    }
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/machinelearning.gaido.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/machinelearning.gaido.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

server {
    if ($host = machinelearning.gaido.dev) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    server_name machinelearning.gaido.dev;
    listen 80;
    return 404; # managed by Certbot
