server {
    server_name static.gaido.dev;
    root /mnt/storage/static;
    index index.html;
    client_max_body_size 2048M;
	location / {
	    try_files $uri $uri/ =404;
	}
    sendfile on;
    sendfile_max_chunk 1m;
    tcp_nopush on;
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/static.gaido.dev/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/static.gaido.dev/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}

